<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_FF1F2532</item>
        <item name="colorPrimaryDark">@color/color_FF1F2532</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="main_tab_tv">
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">9sp</item>
        <item name="android:textColor">@color/color_selector_main_tab</item>
    </style>

    <style name="main_tab_icon">
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_marginTop">9dp</item>
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:background">@null</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>


    <style name="et14">
        <item name="android:textSize">14sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/color_hint</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="et15">
        <item name="android:textSize">15sp</item>
