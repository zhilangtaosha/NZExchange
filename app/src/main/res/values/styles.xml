<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_FF1F2532</item>
        <item name="colorPrimaryDark">@color/color_FF1F2532</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="main_tab_tv">
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">9sp</item>
        <item name="android:textColor">@color/color_selector_main_tab</item>
    </style>

    <style name="main_tab_icon">
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_marginTop">9dp</item>
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:background">@null</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>


    <style name="et14">
        <item name="android:textSize">14sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:textColor">@color/color_FFA3AAB9</item>
        <item name="android:textColorHint">@color/color_FF7F8592</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="tx13_aab9">
        <item name="android:textColor">@color/color_FFA3AAB9</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="tx14_aab9">
        <item name="android:textColor">@color/color_FFA3AAB9</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="tx15_aab9">
        <item name="android:textColor">@color/color_FFA3AAB9</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="tx14_87a8">
        <item name="android:textColor">@color/color_FF6D87A8</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="tx15_87a8">
        <item name="android:textColor">@color/color_FF6D87A8</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="tx16_87a8">
        <item name="android:textColor">@color/color_FF6D87A8</item>
        <item name="android:textSize">16sp</item>
    </style>


    <style name="tx12_8592">
        <item name="android:textColor">@color/color_FF7F8592</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="tx14_8592">
        <item name="android:textColor">@color/color_FF7F8592</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="tx15_8592">
        <item name="android:textColor">@color/color_FF7F8592</item>
        <item name="android:textSize">15sp</item>
    </style>


    <style name="DialogTheme" parent="@android:style/Theme.Dialog">
        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 不透明 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 背景 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 模糊 -->
        <item name="android:backgroundDimEnabled">true</item>
    </style>
</resources>
